       ENTRY   NULL
NULL   TSX     .WRFX,4 
       PAR     NULMSG,,3
       TRA     1,4
NULMSG BCI     /NULL ENTRY POINT/
       ENTRY   KILL
KILL   TIA     =HKILL
       ENTRY   RDFLXA
RDFLXA TIA     =HRDFLXA
       END  
