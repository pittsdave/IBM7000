       LAC     MODE,4        IF MODE=READ OR WRITE                      SRCH0051
       TRA*    *+1,4                                                    SRCH0052
               RET                                                      SRCH0053
               PUT                                                      SRCH0054
               PUT                                                      SRCH0055
               RET                                                      SRCH0056
PUT    TSX     STICKY,4      PUT ENTRY IN STICKY REGS                   SRCH0057
               PUTMOD                                                   SRCH0058
               X                                                        SRCH0059
               0,2                                                      SRCH0060
RET    TSX     POP,4         UNSAVE                                     SRCH0061
