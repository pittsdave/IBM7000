       ENTRY   SRCH                                                     SRCH0008
       EXTERN  PUSH,GETEFA,STICKY,SCNDIR,STICKY,POP                     SRCH0009
SRCH   SCA     XRSH,4        SAVE IRS AND TEMPS                         SRCH0010
       SXD     XRSH,1                                                   SRCH0011
       SXA     XRSH+1,2                                                 SRCH0012
       SXD     XRSH+1,3                                                 SRCH0013
       TSX     PUSH,4                                                   SRCH0014
               XRSH-1,,NSAV                                             SRCH0015
               *                                                        SRCH0016
       LAC     XRSH,4                                                   SRCH0017
       CAL*    1,4           GET MODE                                   SRCH0018
