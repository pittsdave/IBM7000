       REM     FIRST ARG LIST TOO SHORT ERROR
PERF   ERROR   A1LSA2
PERS   ERROR   A2LSA1
       REM
       REM
       REM
       REM MAPCAR(L,F) = (L=0 YIELDS 0,
       REM                F(L) YIELDS 0,
       REM                1 YIELDS MAPCAR(CDR(L),F))
       REM
D      HED
MAPCAR TZE 1,4
       SXD RET,4
       TSX $SAVE,4
       TXL     $END3,,F+2
       STQ F
 MCPR  STO L
       LXD F,4
       TXH *+3,4,10
       TSX F,4
       TRA *+4
       SXD *+2,4
       TSX COMPAT,4
           1,,**
       LXD L,4
       CLA 0,4
       PDX ,4
       PXD ,4
       TNZ     MCPR
       TSX UNSAVE,4
       LXD RET,4
       TRA 1,4
       REM MAPCON(L,F)=
       REM (L=0 YIELDS 0,,1 YIELDS NCONC(F(L),MAPCON(CDR(L),F)))
R      HED
MAPCON TZE 1,4
       SXD MCN5,4
       TSX $SAVE,4
       TXL     $END4,,MCN2+2
       STO MCN3
       STQ MCN4
       LXD MCN4,4
       TXH *+3,4,10
       TSX MCN4,4
       TRA *+4
