       CLA 0,4
       PAX 0,4
 SASP2 TXL SASL1,4,**
 SASP1 TXH SASL1,4,**
       ANA SASDM
       LXA     SAST1,4
       TRA 1,4
       REM
 SASP3 PXD     0,0
       LXD     $ARG3,4
       TXH     *+3,4,10
       LXA     SAST1,4
       TRA     $ARG3
       STZ     $ARG3
       LDQ     $ARG3
       PXD     ,4
       LXA     SAST1,4
       TRA     $APPLY
